{
  "version": 2,
  "crons": [
    {
      "path": "/api/v1/cron/expire-meal?type=1&name=Breakfast",
      "schedule": "30 4 * * *"
    },
    {
      "path": "/api/v1/cron/expire-meal?type=2&name=Lunch",
      "schedule": "30 9 * * *"
    },
    {
      "path": "/api/v1/cron/expire-meal?type=3&name=Dinner",
      "schedule": "30 16 * * *"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Credentials", "value": "true" },
        { "key": "Access-Control-Allow-Origin", "value": "https://mess-mate-jucd.vercel.app" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,OPTIONS,PATCH,DELETE,POST,PUT" },
        { "key": "Access-Control-Allow-Headers", "value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version, Authorization" }
      ]
    }
  ]
}